
class Solution {
    public int findJudge(int n, int[][] trust) {
         if(n ==1)return 1;

         int count [] = new int[n+1];
         for(int[] a : trust){
             count[a[0]]--;
             count[a[1]]++;
         }   

         for(int i = 0 ;i<=n;i++){
             if(count[i] == n-1)
             {
                 return i;
             }
         }
         return -1;
    }
}
